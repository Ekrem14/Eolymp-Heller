#include <bits/stdc++.h>
using namespace std;
int main()
{
	int n, m;cin>>n>>m;
	int a[n][m];
	for(int i = 0;i<n;i++)
	{
		for(int j = 0;j<m;j++)
		{
			cin>>a[i][j];
		}
	}
	int b[n][m];
	for(int i = 0;i<n;i++)
	{
		for(int j = 0;j<m;j++)
		{
			cin>>b[i][j];
		}
	}
	int cem = 0;
	for(int i = 0;i<n;i++)
	{
		for(int j = 0;j<m;j++)
		{
		    if(b[i][j]== 1)
			{
				cem+= a[i][j];
				}	
		}
	}
	cout<<cem;
	
}
