#include <bits/stdc++.h>
using namespace std;
int main()
{
  int n;cin>>n;
  int a[n];
  int zeros = 0;
  for(int i = 0;i<n;i++)
  {
      cin>>a[i];
      if(a[i]==0)
      {
          zeros++;
      }
  }
  for(int i = 0;i<zeros;i++)
  {
      cout<<"0 ";
  }
  int qalan = n-zeros;
  for(int i = 0;i<qalan;i++)
  {
      cout<<"1 ";
  }


}


