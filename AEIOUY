#include <bits/stdc++.h>
using namespace std;
int main()
{
  string s;
  int say = 0;
  getline(cin, s);
  for(int i = 0;i<s.size();i++)
  {
  	if(s[i] == 'a' || s[i] == 'A' || s[i] == 'i' || s[i] == 'I' || s[i] == 'u' || s[i] == 'U' || s[i] == 'e'|| s[i] == 'E'|| s[i] == 'o'|| s[i] == 'O'|| s[i] == 'y' || s[i] == 'Y')
  	{ 
  		say++;
	}
  }
  cout<<say;
}
